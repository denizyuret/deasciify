;;; Emacs Turkish Extension (c) Deniz Yuret, 2006

;;; This is for people trying to type Turkish documents on a U.S.
;;; keyboard.  The latest version is available at:
;;;       http://www.denizyuret.com/turkish

;;; To activate the program first load this file into emacs:
;;;       M-x load-file ENTER turkish.el ENTER
;;; Then turn on the turkish mode:
;;;       M-x turkish-mode

;;; In turkish-mode your words should be automatically corrected
;;; whenever you hit space.  Alternatively you can mark a region and
;;; use M-x turkish-correct-region.  If the program makes a mistake
;;; you can toggle a character's accent using C-t.

;;; The program was inspired by GÃ¶khan TÃ¼r's deasciifier:
;;;       http://www.hlst.sabanciuniv.edu/TL/deascii.html

;;; The program uses decision lists (included at the end of this file)
;;; which was created based on sample Turkish news text using the GPA
;;; algorithm.  For more information on GPA:
;;;       http://www.denizyuret.com/pub/iscis06

;;; Current test set accuracy is approximately 1 error every 100
;;; words.  (Using 400000 training, 100000 validation, 100000 test
;;; tokens) Details on each ambiguous character is given below.
;;; Columns are: (1) ambiguous character, (2) number of instances, (3)
;;; number of errors, (4) error period = (2)/(3), (5) number of rules.
;;;
;;; (c 10503 60 175 1656) 
;;; (g 11316 28 404 433) 
;;; (i 66470 430 154 1679)
;;; (o 17298 84 205 1059)
;;; (s 23410 192 121 2026)
;;; (u 24395 158 154 1581)
;;; (all 153392 952 161 8434) 

;;; Making emacs work with foreign characters is tricky business.  You
;;; need to set the coding system for three things:
;;; (1) file coding system is set with prefer-coding-system.  Each
;;; call adds a coding system at the front of the priority list for
;;; automatic detection.  The last one is used for new files.
;;; (2) set-terminal-coding-system for the terminal output.
;;; (3) set-keyboard-coding system for keyboard input.
;;; The following uses utf-8 as default, but will recognize latin-5
;;; files as well:

(prefer-coding-system 'iso-latin-5)
(prefer-coding-system 'utf-8)
(set-terminal-coding-system 'utf-8)
(set-keyboard-coding-system 'utf-8)

;;; Here is the code:

(define-minor-mode turkish-mode
  "Toggle Turkish mode.
With no argument, this command toggles the mode.  Non-null prefix
argument turns on the mode.  Null prefix argument turns off the mode.

When Turkish mode is enabled, the space, tab, and enter keys correct
the previous word by adding Turkish accents.  For corrections use C-t
to toggle the accent of the character under cursor."
  ;; The initial value.
  nil
  ;; The indicator for the mode line.
  " Turkish"
  ;; The minor mode bindings.
  '((" " . turkish-correct-last-word)
    ("\t" . turkish-correct-last-word)
    ("\C-m" . turkish-correct-last-word)
    ("\C-t" . turkish-toggle-accent))
  )

(defun turkish-correct-last-word ()
  "Adds necessary accents to the last word when a space is typed."
  (interactive)
  (if (= ?\r last-input-event)
      (newline)
    (self-insert-command 1))
  (save-excursion
    (let ((p (point)))
      (turkish-backward-word)
      (turkish-correct-region (point) p))))

(defun turkish-correct-region (start end)
  "Adds necessary accents to the words in the region."
  (interactive "r")
  (save-excursion
    (goto-char start)
    (while (< (point) end)
      (if (turkish-need-correction-p)
	  (turkish-toggle-accent))
      (forward-char))))

(defun turkish-asciify-region (start end)
  "Replaces turkish letters with ascii equivalents in region."
  (interactive "r")
  (save-excursion
    (goto-char start)
    (while (< (point) end)
      (let ((tr (aref turkish-asciify-table (following-char))))
	(if (not tr) (forward-char)
	  (delete-char 1)
	  (insert tr))))))

(defun turkish-toggle-accent ()
  "Adds or removes turkish accent at the cursor."
  (interactive)
  (let ((alt (aref turkish-toggle-accent-table (following-char))))
    (if alt
	(progn 
	  (delete-char 1)
	  (insert alt)
	  (backward-char)))))

(defun turkish-need-correction-p ()
  "Determine if char at cursor needs correction."
  (let* ((ch (following-char))
	 (tr (or (aref turkish-asciify-table ch) ch))
	 (pl (assoc (downcase tr) turkish-pattern-table))
	 (m (and pl (turkish-match-pattern (cdr pl)))))
    ;; if m then char should turn into turkish else stay ascii
    ;; only exception with capital I when we need the reverse
    (if (= ?I tr)
	(if (= ch tr) (not m) m)
      (if (= ch tr) m (not m)))))

(defvar turkish-context-size 10)

(defun turkish-get-context (size)
  (save-excursion
    (let ((s (make-string (1+ (* 2 size)) ?\ ))
	  (p (point))
	  i c x space)
      (aset s size ?\X)
      (setq i (1+ size))
      (while (and (< i (length s)) (not space) (not (eobp)))
	(forward-char)
	(setq c (following-char))
	(setq x (aref turkish-downcase-asciify-table c))
	(if (not x)
	    (if space t
	      (setq i (1+ i))
	      (setq space t))
	  (aset s i x)
	  (setq i (1+ i))
	  (setq space nil)))
      (setq s (substring s 0 i))
      (goto-char p)
      (setq i (- size 1))
      (setq space nil)
      (while (and (>= i 0) (not (bobp)))
	(backward-char)
	(setq c (following-char))
	(setq x (aref turkish-upcase-accents-table c))
	(if (not x)
	    (if space t
	      (setq i (1- i))
	      (setq space t))
	  (aset s i x)
	  (setq i (1- i))
	  (setq space nil)))
      s)))

(defun turkish-match-pattern (dlist)
  (let ((rank (* 2 (hash-table-size dlist)))
	(str (turkish-get-context turkish-context-size))
	(start 0) 
	len s r)
    (setq len (length str))
    (while (<= start turkish-context-size)
      (let ((end (1+ turkish-context-size)))
	(while (<= end len)
	  (setq s (substring str start end))
	  (setq r (gethash s dlist))
	  (if (and r (< (abs r) (abs rank)))
	      (setq rank r))
	  (setq end (1+ end))))
      (setq start (1+ start)))
    (> rank 0)))

(defun turkish-test-buffer ()
  "Test the algorithm on a buffer of correct Turkish text and report percent accuracy."
  (interactive)
  (setq mark-ring nil)
  (set-mark (point))
  (save-excursion
    (let ((cnt (make-vector 128 0))
	  (err (make-vector 128 0))
	  (t0 (float-time))
	  ch tr)
      (goto-char (point-min))
      (while (not (eobp))
	(setq ch (following-char))
	(if (null (aref turkish-toggle-accent-table ch)) t
	  (setq tr (aref turkish-downcase-asciify-table ch))
	  (aset cnt ?a (1+ (aref cnt ?a)))
	  (aset cnt tr (1+ (aref cnt tr)))
	  (if (not (turkish-need-correction-p)) t
	    (push-mark nil t t)
	    (aset err ?a (1+ (aref err ?a)))
	    (aset err tr (1+ (aref err tr)))))
	(forward-char))
      (princ
       (mapcar (lambda (x) 
		 (list (string x) 
		       (aref cnt x)
		       (aref err x)
		       (if (= (aref err x) 0) 0
			   (/ (aref cnt x) (aref err x)))))
	       '(?a ?c ?g ?i ?o ?s ?u)))
      (princ (- (float-time) t0)))))
	
(defun turkish-backward-word ()
  (backward-word 1)
  (while (and (/= ?\  (char-syntax (preceding-char)))
	      (not (bolp)))
    (backward-word 1)))

;; I don't know why there is more than one multibyte representation
;; for Turkish characters but there is.  So we have unibyte latin-5,
;; multibyte latin-5 and multibyte utf-8.

(defvar turkish-char-alist
  '((?c 231 3815 2279)
    (?C 199 3783 2247)
    (?g 191 3824 331839)
    (?G 190 3792 331838)
    (?i 209 3837 331857)
    (?I 208 3805 331856)
    (?o 246 3830 2294)
    (?O 214 3798 2262)
    (?s 255 3838 331903)
    (?S 254 3806 331902)
    (?u 252 3836 2300)
    (?U 220 3804 2268)))

(put 'turkish-char-table 'char-table-extra-slots 0)

