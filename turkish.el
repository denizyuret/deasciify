;;   "Creates a char table from an association list."
;;   (let ((ct (make-char-table 'turkish-char-table)))
;;     (dolist (entry alist ct)
;;       (aset ct (car entry) (cdr entry)))))

(defvar turkish-asciify-table
  (let ((ct (make-char-table 'turkish-char-table)))
    (dolist (lst turkish-char-alist ct)
      (mapc (lambda (x) (aset ct x (car lst))) (cdr lst))))
  "Converts turkish characters into ascii equivalent.")

(defvar turkish-toggle-accent-table
  (let ((ct (make-char-table 'turkish-char-table)))
    (dolist (lst turkish-char-alist ct)
      (aset ct (car lst) (nth 3 lst))
      (mapc (lambda (x) (aset ct x (car lst))) (cdr lst))))
  "Converts turkish characters into ascii equivalent and appropriate
ascii characters to utf-8 turkish accented versions.  This will also
work if the file is latin-5, emacs will automatically convert utf-8
characters to latin-5 when saving.")

(defvar turkish-upcase-accents-table
  (let ((ct (make-char-table 'turkish-char-table))
	(ch ?a))
    (while (<= ch ?z)
      (aset ct ch ch)
      (aset ct (upcase ch) ch)
      (setq ch (1+ ch)))
    (dolist (lst turkish-char-alist)
      (let ((u (upcase (car lst))))
	(mapc (lambda (x) (aset ct x u)) (cdr lst))))
    (aset ct ?i ?i)
    (aset ct ?I ?I)
    (mapc (lambda (x) (aset ct x ?i))
	  (cdr (assoc ?I turkish-char-alist)))
    (mapc (lambda (x) (aset ct x ?I))
	  (cdr (assoc ?i turkish-char-alist)))
    ct)
  "Lowercase the string except for Turkish accented characters which
are converted to uppercase ascii equivalent.  Useful for pattern
matching.  Handles all 3 encodings.  The confusing case of i is as
follows: i => i, Ä° => i, Ä± => I, I => I")
    
(defvar turkish-downcase-asciify-table
  (let ((ct (make-char-table 'turkish-char-table))
	(ch ?a))
    (while (<= ch ?z)
      (aset ct ch ch)
      (aset ct (upcase ch) ch)
      (setq ch (1+ ch)))
    (dolist (lst turkish-char-alist)
      (let ((u (downcase (car lst))))
	(mapc (lambda (x) (aset ct x u)) (cdr lst))))
    ct))

(defun turkish-pattern-hash (alist)
  "Compiles a decision list into a hash where keys are patterns and
values give the rank and the classification of the decision list.  The
sign of a value gives the classification (positive implies t, negative
implies nil), and the absolute value gives the rank (smaller rank
means higher priority)."
  (let ((pt (make-hash-table :test 'equal :size (length alist)))
	(r 1))
    (dolist (entry alist pt)
      (if (not (gethash (car entry) pt))
	  (puthash (car entry) (if (cdr entry) r (- r)) pt))
      (setq r (1+ r)))))

(defun turkish-make-pattern-table (alist)
  "Compiles the turkish pattern alists into hash tables for efficiency"
  (mapcar (lambda (x) (cons (car x) (turkish-pattern-hash (cdr x)))) alist))

